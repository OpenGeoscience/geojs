    "eslint": "^2.10.2",
    "eslint-config-semistandard": "^6.0.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
