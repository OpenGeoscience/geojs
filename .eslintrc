{
    "extends": "semistandard",
    "rules": {
        "indent": [2, 2, {"VariableDeclarator": 2, "SwitchCase": 1}],
        "padded-blocks": 0,
        "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
        "one-var": 0,
        "no-throw-literal": 0,
        "new-cap": 0,
        "spaced-comment": 0,
        "operator-linebreak": 0,
        "key-spacing": 0,
        "no-unneeded-ternary": 0,
        "yoda": 0,
        "no-useless-call": 0
    },
    "env": {
        "browser": true
    },
    "globals": {}
}
